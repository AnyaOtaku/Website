<script type="text/javascript">
     function play(clip){
       var mv = "http://www.youtube.com/v/"+clip+"&autoplay=0&rel=0";
       swfobject.embedSWF(mv, "myContent", "375", "295", "8");
     }
     $(document).ready(function() {
       play('CsGYh8AacgY');
     });
    </script>
